{
  "extends": "oclif",
  "rules": {
    "max-params": "off",
    "no-console": "off",
    "max-depth": "off",
    "one-var": "off",
    "complexity": "off",
    "unicorn/no-process-exit": "off",
    "no-process-exit": "off",
    "no-throw-literal": "off",
    "node/no-unsupported-features": "off",
    "no-warning-comments": "off",
    "semi": [1, "always"]
  }
}
